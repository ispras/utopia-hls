model SourceSink {
  nodetype source  ()                       => (X<1.00> #0 x);
  nodetype sink    (X<1.00> x)              => ();

  graph main {
    chan Y y;

    node source  n1()       => (y);
    node sink    n2(y)      => ();
  }
}